{
  "private": true,
  "name": "minimal-blog",
  "description": "Typography driven, feature-rich blogging theme with minimal aesthetics. Includes tags/categories support and extensive features for code blocks such as live preview, line numbers, and line highlighting.",
  "version": "1.3.16",
  "author": "LekoArts",
  "license": "0BSD",
  "starter-name": "gatsby-starter-minimal-blog",
  "scripts": {
    "build": "gatsby build --prefix-paths",
    "develop": "gatsby develop",
    "develop:cypress": "cross-env CYPRESS_SUPPORT=y yarn develop",
    "build:cypress": "cross-env CYPRESS_SUPPORT=y yarn build",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public -b gh-page"
  },
  "dependencies": {
    "@lekoarts/gatsby-theme-minimal-blog": "^6.2.1",
    "@weknow/gatsby-remark-twitter": "^0.2.3",
    "gatsby": "^5.12.3",
    "gatsby-plugin-feed": "^5.12.0",
    "gatsby-plugin-google-gtag": "^5.12.3",
    "gatsby-plugin-manifest": "^5.12.0",
    "gatsby-plugin-mdx": "^5.12.3",
    "gatsby-plugin-react-helmet": "^6.12.0",
    "gatsby-plugin-sitemap": "^6.12.0",
    "gatsby-plugin-twitter": "^5.12.3",
    "gatsby-plugin-webpack-statoscope": "^1.0.3",
    "gatsby-remark-katex": "^4.2.0",
    "gatsby-remark-link-beautify": "^2.2.6",
    "gatsby-remark-link-card": "^1.0.3",
    "gatsby-remark-link-preview": "^1.0.1",
    "gatsby-transformer-remark": "^6.12.3",
    "html-react-parser": "^5.0.6",
    "katex": "^0.16.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "remark-html-katex": "^4.0.1",
    "remark-math": "^6.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.17.12",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "cross-env": "^7.0.3",
    "typescript": "^5.2.2"
  }
}
